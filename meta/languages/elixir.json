{
  "meta": {
    "keywords": "and false then defined module in return redo retry end for true self when next until do begin unless nil break not case cond alias while ensure or include use alias fn quote"
  },
  "root": {
    "contains": [
      {
        "className": "ident",
        "begin": "[a-zA-Z_][a-zA-Z0-9_]*(\\!|\\?)?"
      },
      {
        "className": "comment",
        "begin": "#",
        "end": "$"
      },
      {
        "className": "number",
        "begin": "(\\b0[0-7_]+)|(\\b0x[0-9a-fA-F_]+)|(\\b[1-9][0-9_]*(\\.[0-9_]+)?)|[0_]\\b"
      },
      {
        "className": "string",
        "contains": [
          {
            "begin": "\\\\[\\s\\S]"
          }
        ],
        "begin": "'",
        "end": "'"
      },
      {
        "className": "string",
        "contains": [
          {
            "begin": "\\\\[\\s\\S]"
          }
        ],
        "begin": "\"",
        "end": "\""
      }
    ]
  }
}